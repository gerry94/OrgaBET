<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="374.0" prefWidth="228.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LibrarianUIController">
  <children>
    <AnchorPane fx:id="output_txt" maxHeight="-1.0" maxWidth="-1.0" prefHeight="374.0" prefWidth="555.0" VBox.vgrow="ALWAYS">
      <children>
            <Text layoutX="70.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="BibliOS">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Text>
            <Text fx:id="welcome_msg" layoutX="35.0" layoutY="94.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, &lt;username&gt;" />
            <Button fx:id="logout_but" layoutX="39.0" layoutY="107.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
            <Button fx:id="shutdown_but" layoutX="114.0" layoutY="107.0" mnemonicParsing="false" text="Shutdown" />
            <Button fx:id="book_but" layoutX="50.0" layoutY="168.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="125.0" onAction="#books" text="Manage Books" />
            <Button fx:id="loan_but" layoutX="50.0" layoutY="232.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="125.0" onAction="#loans" text="Manage Loans" />
            <Button fx:id="user_but" layoutX="50.0" layoutY="292.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="125.0" onAction="#users" text="Manage Users" />
            <Text layoutX="50.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Administration Panel">
               <font>
                  <Font size="12.0" />
               </font>
            </Text>
      </children>
    </AnchorPane>
  </children>
</VBox>
